<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#A9D0E1"
        android:orientation="vertical"
        android:padding="30dp"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="365dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="App programming 다시보기"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp" />

        <Button
            android:id="@+id/week9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week 9"
            app:backgroundTint="#495155" />

        <Button
            android:id="@+id/week10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week 10"
            app:backgroundTint="#495155" />

        <Button
            android:id="@+id/week11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week 11"
            app:backgroundTint="#495155" />

        <Button
            android:id="@+id/week12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week 12"
            app:backgroundTint="#495155" />

        <Button
            android:id="@+id/week13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week13"
            app:backgroundTint="#495155" />

        <Button
            android:id="@+id/week14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="week 14"
            app:backgroundTint="#495155" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>